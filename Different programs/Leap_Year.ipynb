      "source": [
        "year = int(input(\"Enter year: \"))\n",
        "if(year%4 == 0):\n",
        "  if(year%100 == 0):\n",
        "    if(year%400 == 0):\n",
        "      print(\"Its a leap year\")\n",
        "    else:\n",
        "      print(\"Not a leap year\")\n",
        "  else:\n",
        "    print(\"Its a leap year\")\n",
        "else:\n",
        "  print(\"Not a  leap year\")   "
